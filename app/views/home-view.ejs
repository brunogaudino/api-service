<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Home page</title>
    <style>
        body{
            background: #C0C0C0;
        }
        h1{
            margin: 10px 0 20px 0;
            text-align: center;
        }
        table{
            width: 100%;
            text-align: left;
            border: 1px solid whitesmoke;
            border-spacing: 0;
        }
        th, td{
            border: 1px solid whitesmoke;
            padding: 10px;
        }
        th{
            background-color: #00233B;
            color: white;
        }
        tr:nth-child(even) {
            background-color: #dcdcdc;
        }
        tr:nth-child(odd) {
            background-color: #a2a2a2;
        }

        #signature{
            margin: 10px 0;
            padding: 10px 0;
            text-align: left;
            font-weight: 700;
            text-align: center;
            border-top: 1px solid white;
            border-bottom: 1px solid white;
        }
        #signature a{
            color: black;
        }

    </style>
</head>
<body>
    <h1>Data list</h1>
    <table>
        <thead>
            <th>Name</th>
            <th>E-mail</th>
        </thead>
        <tbody>
        <% for ( result of repository){%>
            <tr>
                <td><%= result.name%></td>
                <td><%= result.email%></td>
            </tr>
        <%}%>
        </tbody>
    </table>
    <div id="signature">
        Develop by <a href="http://brunogaudino.github.io" target="_blank">Bruno Gaudino</a>
    </div>
</body>
</html>